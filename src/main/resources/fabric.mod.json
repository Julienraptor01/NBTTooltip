{
  "schemaVersion": 1,
  "id": "nbttooltip",
  "version": "${version}",
  "name": "NBT Tooltip",
  "description": "Add a handy tooltip to items, showing NBT data",
  "authors": [
    "Zabi94"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/nbttooltip",
    "sources": "https://github.com/zabi94/NBTTooltip",
    "issues": "https://github.com/zabi94/NBTTooltip/issues"
  },
  "license": "MIT",
  "icon": "assets/nbttooltip/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "zabi.minecraft.nbttooltip.NBTTooltip"
    ],
    "modmenu": [
      "zabi.minecraft.nbttooltip.config.ConfigScreenProvider"
    ]
  },
  "depends": {
  "fabricloader": ">=${fabricloaderversion}",
  "minecraft": "~${mcversion}",
  "fabric-api": ">=${fabricapiversion}",
  "cloth-config2": ">=${clothconfigversion}"
  },
  "recommends": {
    "modmenu": ">=${modmenuversion}"
  }
}
